FoodWebBuilder::Application.routes.draw do
		#Load resources for models
	
	resource :authors
	resource :author_cites
	resource :citation
	resource :competition_interation
	resource :compeition_interaction_observation
	resource :faciliation_interaction
	resource :facilitation_interaction_observation
	resource :functional_group
	resource :location
	resource :location_data
	resource :node
	resource :node_max_age
	resource :node_range
	resource :non_itis
	resource :parasitic_interaction
	resource :parasitic_interaction_observation
	resource :stage
	resource :stage_biomass_change
	resource :stage_biomass_density
	resource :stage_consume_biomass_ratio
	resource :stage_consumer_strategy
	resource :stage_drymass
	resource :stage_duration
	resource :stage_fecundity
	resource :stage_habitat
	resource :stage_habitat_descriptor
	resource :stage_length
	resource :stage_length_fecundity
	resource :stage_length_weight
	resource :stage_lifestyle
	resource :stage_masses
	resource :stage_max_depth
	resource :stage_mobility
	resource :stage_populations
	resource :stage_prod_biomass_ratio
	resource :stage_prod_consum_ratio
	resource :stage_reproductive_strategy
	resource :stage_residency
	resource :stage_residency_time
	resource :stage_unassimilated_consum_ratio
	resource :trophic_interaction
	resource :trophic_interaction_observation

	# Mount the Rails Admin engine
	mount RailsAdmin::Engine => '/admin', :as => 'rails_admin'
	# Configure routes for authenticated User to dashboard
  authenticated :user do
  	root :to => 'dashboard#index' 
  end
  # Default root to home/index.html.erb
  root :to =>"home#index"
  devise_for :users
  resources :users
  
  # add some matching to make coding easier
  match 'about' => 'home#about'
  match 'help' => 'home#help'
  match 'visualization' => 'visualization#index'
  match 'dataentry' => 'dashboard#dataentry'
	

end
